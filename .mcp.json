{
  "mcpServers": {
    "playwright-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "big-brain": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@probelabs/big-brain@latest",
        "--chatgpt"
      ],
      "env": {}
    }
  }
}